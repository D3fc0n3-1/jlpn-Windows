<Window x:Class="JNLP_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="JNLP Launcher" Height="100" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="UrlTextBox" Grid.Row="0" Margin="10"/>
        <Button x:Name="LaunchButton" Grid.Row="1" Margin="10" Content="Launch" Click="LaunchButton_Click"/>
    </Grid>
</Window>